<table class="stats">
  <tr class="headerrow">
    <th>Nickname</th>
    <th>Receive notifications on new tickets and requests</th>
  <tr>
  <% @admins.each do |admin| %>
    <tr>
      <td><%=admin.nickname%></td>
      <td><%=check_box_tag :send_admin_notifications, "1", admin.send_admin_notifications, :onclick => "toggleAdminNotifications(#{admin.id})" %> </td>
    </tr>
  <% end %>
</table>
