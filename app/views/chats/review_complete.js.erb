<% if @warning %>
  alert("<%= @warning %>");
  <% if @bid && @bid.managed_work && @bid.managed_work.complete? %>
    location.reload();
  <% end %>
<% else %>
  jQuery('#review_bids').html('<%= escape_javascript(render(partial: 'bids_to_review').squish.html_safe) %>').show().effect('highlight', {}, 3000);
  jQuery("#lang<%= @lang_id %>").html('<%= escape_javascript(render(partial: 'show_bid', object: @bid_disp).squish.html_safe) %>');
<% end %>